programa
{
	funcao inicio()
	{
		real numero1, numero2 
		escreva("Digite o primeiro número (A): ")
		leia(numero1)
		
		escreva("Digite o segundo número (B): ")
		leia(numero2)
		
		escreva("\n") 
		se (numero1 > numero2)
		{
			escreva("O MAIOR número é o primeiro: ", numero1)
		}
		senao se (numero2 > numero1)
		{
			escreva("O MAIOR número é o segundo: ", numero2)
		}
		senao 
		{
			escreva("Os dois números são IGUAIS: ", numero1)
		}
	}
}
